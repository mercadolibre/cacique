.info{:id=>"infoCategory", :style=>"display:none;left:310px;"}
  =render :partial =>"layouts/popup_header", :locals=>{:element =>"infoCategory"} 
  .infoData
    %h2{:style=>"text-align:center;"}
      %b
        =_('Folder')
      %p
        =_('Each folder is a group of scripts.')
